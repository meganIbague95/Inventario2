                        this.iframe.style.position = "inherit";
                        adaptors.logger.log('the ad might look awful', slotStylePosition);
                        break;
                }
            }
            catch (er) {
                adaptors.logger.error('adapi cant get slot computed style or can\'t modify slot style');
            }
