r\n//     _____                               _ \r\n//    / ____|                             | |\r\n//   | |     __ _ _ __ ___  _   _ ___  ___| |\r\n//   | |    / _` | '__/ _ \\| | | / __|/ _ \\ |\r\n//   | |___| (_| | | | (_) | |_| \\__ \\  __/ |\r\n//    \\_____\\__,_|_|  \\___/ \\__,_|___/\\___|_|\r\n//                                           \r\n//     \r\n\r\nvar phoneOrientation = 0; // https://compat.spec.whatwg.org/#event-orientationchange\r\naddEventListener(\"orientationchange\", function (evt) {\r\n    phoneOrientation = screen.orientation.angle == 0 ? 0 : 1;\r\n    TSCreativeScale.updateScale();\r\n});\r\n\r\nvar TSCarousel = function (config) {\r\n    var self = this;\r\n\r\n    self.config = config;\r\n    self.trackLoop = false;\r\n\r\n    self.initialized = false;\r\n    self.initialize = function () {\r\n        if (self.initialized) return;\r\n        self.initialized = true;\r\n\r\n        self.slides.init();\r\n        self.arrows.init();\r\n    }\r\n\r\n    self.started = false;\r\n    self.start = function () {\r\n        if (self.started) return;\r\n        self.started = true;\r\n\r\n        self.locked = false;\r\n        self.slides.updatePositions(bnt.get(bnt.State));\r\n        self.slides.activateSlideDrag();\r\n\r\n        self.setAutoAdvance();\r\n\r\n        if (!self.monitored) self.monitoring();\r\n\r\n        self.slides.trackSlide(self.slides.currentIndex);\r\n    }\r\n\r\n    self.autoAdvanceInterval = null;\r\n    self.setAutoAdvance = function () {\r\n        self.config.autoAdvance && (self.autoAdvanceInterval = setInterval(self.showNextSlide, self.config.autoAdvance * 1000));\r\n        addEventListener('click', self.restartAutoAdvance);\r\n        addEventListener('mousedown', self.restartAutoAdvance);\r\n        addEventListener('touchstart', self.restartAutoAdvance);\r\n    };\r\n\r\n    self.showNextSlide = function () {\r\n        if (document.visibilityState == 'hidden' || self.paused) return\r\n        self.trackFromUserInput = false;\r\n        self.controls.next();\r\n    };\r\n\r\n    self.autoResumeTimeout = null;\r\n    self.restartAutoAdvance = function (evt) {\r\n        clearInterval(self.autoAdvanceInterval);\r\n        clearTimeout(self.autoResumeTimeout);\r\n\r\n        if (self.config.autoResume && !self.slides.pressed) {\r\n            self.autoResumeTimeout = setTimeout(function () {\r\n                if (self.config.autoAdvance) {\r\n                    self.autoAdvanceInterval = setInterval(self.showNextSlide, self.config.autoAdvance * 1000);\r\n                    self.showNextSlide();\r\n                }\r\n            }, self.config.autoResume * 1000);\r\n        }\r\n    };\r\n\r\n    self.monitoring = function () {\r\n        bnt.logger.log('MONITORING');\r\n        self.monitored = true;\r\n\r\n        var loadingImagesPRs = [];\r\n        var monitorImage = function (imageEl) {\r\n            var imageHtmlEl = bnt.ElementRendererRegistry.rendererFor(imageEl).imageElement;\r\n            if (imageHtmlEl.src && !imageHtmlEl.complete) {\r\n                var pr = new bnt.Promise()\r\n                var onComplete = function (evt) {\r\n                    imageHtmlEl.removeEventListener('load', onComplete);\r\n                    imageHtmlEl.removeEventListener('error', onComplete);\r\n                    pr.fulfill(null)\r\n                }\r\n                imageHtmlEl.addEventListener('load', onComplete);\r\n                imageHtmlEl.addEventListener('error', onComplete)\r\n                loadingImagesPRs.push(pr.future)\r\n            }\r\n        }\r\n\r\n        // monitor arrows\r\n        self.config.arrows && Object.keys(self.config.arrows)\r\n            .map(function (key) {\r\n                return self.config.arrows[key]\r\n            }).forEach(monitorImage);\r\n        // monitor necesary slides\r\n        self.slides.list.slice(0, self.config.necessarySlides)\r\n            .forEach(function (necessaryGroupLoad) {\r\n                necessaryGroupLoad.element.deepGetEosByType(bnt.Image).map(function (eos) {\r\n                    return eos.element\r\n                }).forEach(monitorImage)\r\n            })\r\n\r\n        bnt.Promise.forAll(loadingImagesPRs).map(function () {\r\n            adApi.st.sumologic.track('d0', 'carousel-l');\r\n        })\r\n    };\r\n\r\n    self.arrows = {\r\n        groupLeft: null,\r\n        groupRight: null,\r\n\r\n        init: function () {\r\n            /* extract parent group of image to show/hide group element */\r\n            self.arrows.groupRight = self.config.screen.deepGetEosByType(bnt.Group)\r\n                .map(function (eos) {\r\n                    return eos.element\r\n                })\r\n                .find(function (el) {\r\n                    return (el.eos.filter(function (eos) {\r\n                        return eos.element == self.config.arrows.next\r\n                    }).length > 0)\r\n                });\r\n            self.arrows.groupLeft = self.config.screen.deepGetEosByType(bnt.Group)\r\n                .map(function (eos) {\r\n                    return eos.element\r\n                })\r\n                .find(function (el) {\r\n                    return (el.eos.filter(function (eos) {\r\n                        return eos.element == self.config.arrows.previous\r\n                    }).length > 0)\r\n                });\r\n            /* end of extraction */\r\n\r\n            if (!self.config.arrows) {\r\n                bnt.get(bnt.StateChangeDetector).stateUpdated.addObserver(function () {\r\n                    self.arrows.groupLeft && self.arrows.groupLeft.update({ visible: false })\r\n                    self.arrows.groupRight && self.arrows.groupRight.update({ visible: false })\r\n                });\r\n                return;\r\n            }\r\n\r\n            if (self.arrows.groupLeft) {\r\n                self.arrows.groupLeft.show();\r\n                self.config.arrows.previous.htmlElement.style.cursor = 'pointer';\r\n                self.config.arrows.previous.onclick.addObserver(function () {\r\n                    if (!self.locked) {\r\n                        self.trackFromUserInput = true;\r\n                        self.controls.previous();\r\n                    }\r\n                });\r\n            }\r\n\r\n            if (self.arrows.groupRight) {\r\n                self.arrows.groupRight.show();\r\n                self.config.arrows.next.htmlElement.style.cursor = 'pointer';\r\n                self.config.arrows.next.onclick.addObserver(function () {\r\n                    if (!self.locked) {\r\n                        self.trackFromUserInput = true;\r\n                        self.controls.next();\r\n                    }\r\n                });\r\n            }\r\n\r\n            if (!self.config.loop) {\r\n                self.arrows.groupLeft && self.arrows.groupLeft.hide();\r\n            }\r\n\r\n        },\r\n\r\n        update: function () {\r\n            if (!self.config.arrows || self.config.loop) return;\r\n\r\n            if (self.arrows.groupLeft) {\r\n                if (self.slides.currentIndex == 0) self.arrows.groupLeft.hide();\r\n                else self.arrows.groupLeft.show();\r\n            }\r\n\r\n            if (self.arrows.groupRight) {\r\n                if (self.slides.currentIndex == self.slides.list.length - 1) self.arrows.groupRight.hide();\r\n                else self.arrows.groupRight.show();\r\n            }\r\n        }\r\n    }\r\n\r\n    self.slides = {\r\n        list: [],\r\n        wrapper: null,\r\n        currentIndex: 0,\r\n        deltaIndexes: [0, -1, 1],\r\n        changed: new bnt.Observable(),\r\n        widthWithMargin: null,\r\n\r\n        init: function () {\r\n            if (self.config.screen) self.slides.wrapper = self.config.screen.deepGetEos(self.config.container);\r\n            else {\r\n                creative.forEachScreen(function (screen) {\r\n                    var w = screen.deepGetEos(self.config.container);\r\n\r\n                    if (w) self.slides.wrapper = w;\r\n                });\r\n            }\r\n\r\n            self.slides.list = self.slides.wrapper.element.eos;\r\n\r\n            self.slides.widthWithMargin = self.config.slide.sizes.width + self.config.slide.sizes.margin;\r\n\r\n            self.slides.changed.addObserver(function (index) {\r\n                self.slides.list.forEach(function (slide, idx) {\r\n                    var activateIndex = index || self.slides.currentIndex\r\n                    slide.element.htmlElement.style.opacity = (idx != activateIndex) ?\r\n                        self.config.slide.inactive.opacity : 1;\r\n                })\r\n            })\r\n        },\r\n\r\n        forEachSlide: function (fn) {\r\n            for (var i = 0; i < self.slides.list.length; i++) fn(self.slides.list[i], i, self.slides.list);\r\n        },\r\n\r\n        trackSlide: function (index) {\r\n            if (self.slides.list[index].tracked) return;\r\n            self.slides.list[index].tracked = true;\r\n            bntTracking.trackEvent(\"+Slide \" + self.slides.list[index].element.elementId + \" in view\", null, false);\r\n        },\r\n\r\n\r\n        updatePositions: function (currentState) {\r\n            self.canvasConfig = currentState.canvas.config;\r\n            self.canvasWidth = self.canvasConfig.width;\r\n            self.canvasHeight = self.canvasConfig.height;\r\n            self.halfXDifference = (self.canvasWidth - self.slides.widthWithMargin) / 2;\r\n            self.containerWidth = (self.slides.widthWithMargin) * self.slides.list.length;\r\n\r\n            self.slides.forEachSlide(function (slide, i) {\r\n                var config = slide.getConfig(currentState);\r\n\r\n                slide.element.htmlElement.transition = 'none';\r\n\r\n                config.x = (i == 0) ?\r\n                    self.config.slide.sizes.margin / 2 :\r\n                    self.config.slide.sizes.margin / 2 + (self.slides.widthWithMargin) * i;\r\n                config.y = 0;\r\n\r\n                (self.config.loop && (i == self.slides.list.length - 1)) && (config.x = self.config.slide.sizes.margin / 2 - self.slides.widthWithMargin);\r\n\r\n                bnt.ElementRendererRegistry.rendererFor(slide.element).applyScreenConfig(currentState, config);\r\n            });\r\n\r\n            var configWrapper = self.slides.wrapper.getConfig(currentState);\r\n            configWrapper.x = (self.config.loop) ? self.halfXDifference : 0;\r\n            bnt.ElementRendererRegistry.rendererFor(self.slides.wrapper.element).applyScreenConfig(currentState, configWrapper);\r\n\r\n            self.slides.changed.update();\r\n        },\r\n\r\n        normalizeDeltaIndex: function (D) {\r\n            var sIndex = self.slides.currentIndex + D;\r\n            if (self.slides.currentIndex + D >= self.slides.list.length) {\r\n                sIndex = self.slides.currentIndex + D - self.slides.list.length;\r\n            } else if (self.slides.currentIndex + D < 0) {\r\n                sIndex = self.slides.currentIndex + D + self.slides.list.length\r\n            }\r\n\r\n            return sIndex;\r\n        },\r\n\r\n        getIndexFromPosition: function () {\r\n            self.slides.containerLeft = parseInt(self.config.container.htmlElement.style.left, 10);\r\n            return Math.round(self.slides.containerLeft * (self.slides.list.length - 1) / (self.canvasWidth - self.containerWidth));\r\n        },\r\n\r\n        getPositionFromIndex: function () {\r\n            if (self.slides.currentIndex == 0) {\r\n                return 0;\r\n            } else if (self.slides.currentIndex == self.slides.list.length - 1) {\r\n                return self.canvasWidth - self.containerWidth;\r\n            } else {\r\n                return self.halfXDifference - self.slides.currentIndex * self.slides.widthWithMargin;\r\n            }\r\n        },\r\n\r\n        activateSlideDrag: function () {\r\n            var target = bnt.ElementRendererRegistry.rendererFor(self.slides.wrapper.element).htmlElement;\r\n\r\n            /* Mouse events */\r\n            target.addEventListener('mousedown', self.slides.dragStart);\r\n            target.addEventListener('mousemove', self.slides.dragMove);\r\n            target.addEventListener('mouseup', self.slides.dragStop);\r\n            target.addEventListener('mouseout', self.slides.dragStop);\r\n\r\n            /* Touch events */\r\n            target.addEventListener(\"touchstart\", self.slides.dragStart);\r\n            target.addEventListener(\"touchmove\", self.slides.dragMove);\r\n            target.addEventListener(\"touchend\", self.slides.dragStop);\r\n            target.addEventListener(\"touchcancel\", self.slides.dragStop);\r\n        },\r\n\r\n        dragStart: function (evt) {\r\n            if (self.locked) return;\r\n            self.locked = true;\r\n            self.slides.pressed = true;\r\n\r\n            self.slides.oldIndex = self.slides.currentIndex;\r\n            self.config.container.htmlElement.style.transition = 'none';\r\n            !self.config.loop && (self.slides.containerStartingLeft = parseInt(self.config.container.htmlElement.style.left, 10));\r\n\r\n            TSCreativeScale.updateScale();\r\n\r\n            self.slides.dragOffset = evt.clientX || event.touches[0].clientX;\r\n        },\r\n\r\n        dragMove: function (evt) {\r\n            if (!self.slides.pressed) return;\r\n\r\n            self.slides.newOffset = evt.clientX || evt.touches[0].clientX;\r\n            self.slides.delta = self.slides.newOffset - self.slides.dragOffset;\r\n\r\n            Math.abs(self.slides.delta) != 0 && evt.preventDefault();\r\n\r\n            self.slides.dragOffset = self.slides.newOffset;\r\n            self.slides.movingDirection = self.slides.delta > 0 ? -1 : 1;\r\n            self.slides.deltaIndexes = [0, -1, 1, (2 * self.slides.movingDirection)];\r\n            self.slides.moveDelta = phoneOrientation == 0 ?\r\n                self.slides.delta * TSCreativeScale.invScale :\r\n                self.slides.delta / TSCreativeScale.scale;\r\n            self.config.loop ?\r\n                self.slides.dragSlides(self.slides.moveDelta) :\r\n                self.slides.dragContainer(self.slides.moveDelta)\r\n        },\r\n\r\n        dragStop: function (evt) {\r\n            if (!self.slides.pressed) return;\r\n            self.slides.pressed = false;\r\n\r\n            (self.config.loop) ?\r\n                self.slides.snapSlides() :\r\n                self.slides.snapContainer()\r\n\r\n            if (self.slides.oldIndex != self.slides.currentIndex) {\r\n                self.slides.trackSlide(self.slides.currentIndex);\r\n                bntTracking.trackEvent(\"Swiped to slide \" + self.slides.list[self.slides.currentIndex].element.elementId, null, false);\r\n            }\r\n\r\n            self.controls.setUnlockTimeout();\r\n            self.restartAutoAdvance();\r\n        },\r\n\r\n        releaseMouseMove: function () {\r\n            var evtUp = document.createEvent('MouseEvents');\r\n            evtUp.initMouseEvent('mouseup', true, true);\r\n            bnt.ElementRendererRegistry.rendererFor(self.slides.wrapper.element).htmlElement.dispatchEvent(evtUp);\r\n        },\r\n\r\n        dragContainer: function (distance) {\r\n            self.slides.containerLeft = parseInt(self.config.container.htmlElement.style.left, 10)\r\n\r\n            if (self.slides.containerLeft + distance <= 0 && self.slides.containerLeft + distance >= self.canvasWidth - self.containerWidth) {\r\n                (self.config.container.htmlElement.style.left = (self.slides.containerLeft + distance) + 'px')\r\n            }\r\n\r\n            /* release for snap checks */\r\n            if (self.slides.getIndexFromPosition() != self.slides.oldIndex) {\r\n                self.slides.changed.update(self.slides.getIndexFromPosition());\r\n                // self.slides.releaseMouseMove();\r\n            }\r\n        },\r\n\r\n        dragSlides: function (distance) {\r\n            var sign = self.slides.deltaIndexes[3] > 0 ? -1 : 1;\r\n\r\n            self.slides.deltaIndexes.forEach(function (D, i) {\r\n                var sIndex = self.slides.normalizeDeltaIndex(D);\r\n\r\n                var slideElement = self.slides.list[sIndex].element;\r\n                var currentLeft = parseInt(slideElement.htmlElement.style.left, 10);\r\n\r\n                slideElement.htmlElement.style.transition = \"none\";\r\n                slideElement.htmlElement.style.transform = \"none\";\r\n                slideElement.htmlElement.style.left = i == 0 ?\r\n                    Math.floor(currentLeft + distance) + 'px' :\r\n                    parseInt(self.slides.list[self.slides.currentIndex].element.htmlElement.style.left, 10) + (self.slides.widthWithMargin * D) + 'px'\r\n            })\r\n\r\n            /* release for snap checks */\r\n            var currentX = self.slides.list[self.slides.currentIndex].element.htmlElement.getBoundingClientRect().x ||\r\n                self.slides.list[self.slides.currentIndex].element.htmlElement.getBoundingClientRect().left;\r\n            var currentWidth = self.slides.widthWithMargin;\r\n            var scaledChangeLimit = (self.slides.widthWithMargin * 2 - self.canvasWidth) / 2 * TSCreativeScale.scale;\r\n\r\n            if (currentX < - scaledChangeLimit + (self.config.slide.sizes.margin * TSCreativeScale.scale / 2)) {\r\n                self.slides.currentIndex++;\r\n                self.slides.clampCurrentIndex();\r\n                self.slides.changed.update();\r\n                // self.slides.releaseMouseMove();\r\n            }\r\n            if (currentX + currentWidth * TSCreativeScale.scale > self.canvasWidth * TSCreativeScale.scale + scaledChangeLimit + (self.config.slide.sizes.margin * TSCreativeScale.scale / 2)) {\r\n                self.slides.currentIndex--;\r\n                self.slides.clampCurrentIndex();\r\n                self.slides.changed.update();\r\n                // self.slides.releaseMouseMove();\r\n            }\r\n        },\r\n\r\n        snapContainer: function () {\r\n            self.slides.currentIndex = self.slides.getIndexFromPosition();\r\n            self.slides.animateContainer();\r\n            self.slides.changed.update();\r\n        },\r\n\r\n        snapSlides: function () {\r\n            (self.slides.oldIndex == self.slides.list.length - 1 && self.slides.currentIndex == 0) && bntTracking.trackEvent(\"+Looped\", null, false);\r\n            self.slides.deltaIndexes.forEach(function (D, i) {\r\n                var sIndex = self.slides.normalizeDeltaIndex(D);\r\n\r\n                /* put slides in right positions with right transition duration */\r\n                var slideElement = self.slides.list[sIndex].element;\r\n                var currentLeft = parseInt(slideElement.htmlElement.style.left, 10);\r\n                var moveDistance = Math.abs(currentLeft - (D * self.slides.widthWithMargin));\r\n\r\n                (i != 3) && (self.slides.transitionDuration = (moveDistance * self.config.transitionSpeed / self.slides.widthWithMargin));\r\n                slideElement.htmlElement.style.transition = Math.abs(D) != 2 ?\r\n                    self.slides.transitionDuration / 1000 + \"s\" : '0s'\r\n\r\n                slideElement.htmlElement.style.left = Math.floor(self.config.slide.sizes.margin / 2 + (D * self.slides.widthWithMargin)) + 'px';\r\n            })\r\n            self.slides.changed.update();\r\n        },\r\n\r\n        clampCurrentIndex: function () {\r\n            self.slides.currentIndex == self.slides.list.length && (self.slides.currentIndex = 0);\r\n            self.slides.currentIndex < 0 && (self.slides.currentIndex = self.slides.list.length - 1);\r\n        },\r\n\r\n        animateSlides: function () {\r\n            var sign = self.slides.deltaIndexes[3] > 0 ? -1 : 1;\r\n            /* put mobile slide in right position */\r\n            var mobileSlideIdx = self.slides.normalizeDeltaIndex(sign);\r\n            self.slides.list[mobileSlideIdx].element.htmlElement.style.transition = \"none\";\r\n            self.slides.list[mobileSlideIdx].element.htmlElement.style.left = 2 * sign * self.slides.widthWithMargin;\r\n\r\n            /* timeout needed so mobile slide is animated without transition */\r\n            setTimeout(function () {\r\n                self.slides.deltaIndexes.forEach(function (D, i) {\r\n                    var sIndex = self.slides.normalizeDeltaIndex(D);\r\n\r\n                    var slideElement = self.slides.list[sIndex].element;\r\n                    var currentLeft = parseInt(slideElement.htmlElement.style.left, 10);\r\n                    var moveDistance = Math.abs(currentLeft - (D * self.slides.widthWithMargin));\r\n                    self.slides.transitionDuration = self.config.transitionSpeed;\r\n\r\n                    slideElement.htmlElement.style.transition = self.slides.transitionDuration / 1000 + \"s\";\r\n                    slideElement.htmlElement.style.left = i == 0 ?\r\n                        0 + 'px' :\r\n                        Math.floor(parseInt(self.slides.list[self.slides.currentIndex].element.htmlElement.style.left, 10) + (self.slides.widthWithMargin * D)) + 'px'\r\n                })\r\n            }, 32)\r\n        },\r\n\r\n        animateContainer: function () {\r\n            var containerLeft = self.slides.getPositionFromIndex();\r\n\r\n            var moveDistance = Math.abs(parseInt(self.config.container.htmlElement.style.left, 10) - containerLeft);\r\n            self.slides.transitionDuration = (moveDistance * self.config.transitionSpeed / self.slides.widthWithMargin);\r\n\r\n            self.config.container.htmlElement.style.transition = (self.slides.transitionDuration / 1000) + \"s\";\r\n            self.config.container.htmlElement.style.left = containerLeft + 'px';\r\n        }\r\n    }\r\n\r\n    self.controls = {\r\n        canGoToPreviousSlide: function () { return (self.config.loop || self.slides.currentIndex > 0); },\r\n        canGoToNextSlide: function () { return (self.config.loop || self.slides.currentIndex < self.slides.list.length - 1); },\r\n        next: function () {\r\n            if (self.controls.canGoToNextSlide()) {\r\n                self.slides.deltaIndexes = [0, -1, 1, -2]\r\n                if (self.config.loop && (self.slides.currentIndex == self.slides.list.length - 1)) {\r\n                    self.controls.to(0);\r\n                } else {\r\n                    self.controls.to(self.slides.currentIndex + 1);\r\n                    self.trackLoop = true;\r\n                }\r\n            }\r\n        },\r\n        previous: function () {\r\n            if (self.controls.canGoToPreviousSlide()) {\r\n                self.slides.deltaIndexes = [0, -1, 1, 2]\r\n                if (self.config.loop && (self.slides.currentIndex - 1 < 0))\r\n                    self.controls.to(self.slides.list.length - 1);\r\n                else\r\n                    self.controls.to(self.slides.currentIndex - 1);\r\n            }\r\n        },\r\n\r\n        to: function (slideIndex) {\r\n            if (self.locked) return;\r\n            self.locked = true;\r\n\r\n            self.slides.currentIndex = slideIndex;\r\n            (self.config.loop) ?\r\n                self.slides.animateSlides() :\r\n                self.slides.animateContainer();\r\n\r\n            self.slides.changed.update();\r\n            self.controls.setUnlockTimeout();\r\n\r\n            self.trackFromUserInput && bntTracking.trackEvent(\"Clicked to slide \" + self.slides.list[self.slides.currentIndex].element.elementId, null, false);\r\n        },\r\n\r\n        setUnlockTimeout: function () {\r\n            clearTimeout(self.controls.unlockTO);\r\n            self.controls.unlockTO = setTimeout(function () {\r\n                self.locked = false;\r\n                self.arrows.update();\r\n                self.slides.trackSlide(self.slides.currentIndex);\r\n                self.slides.changed.update();\r\n                self.config.loop && (self.slides.currentIndex == self.slides.list.length - 1) && self.trackLoop && bntTracking.trackEvent(\"+Looped\", null, false);\r\n                self.trackLoop = false;\r\n            }, self.slides.transitionDuration || self.config.transitionSpeed);\r\n        }\r\n    }\r\n};\r\n\r\nfunction updatePositionsForPrerender(currentState) {\r\n    var widthWithMargin = TSCarouselConfig.slide.sizes.width + TSCarouselConfig.slide.sizes.margin;\r\n    var canvasConfig = currentState.canvas.config;\r\n    var canvasWidth = canvasConfig.width;\r\n    var halfXDifference = (canvasWidth - widthWithMargin) / 2;\r\n    var wrapper;\r\n    if (TSCarouselConfig.screen) wrapper = TSCarouselConfig.screen.deepGetEos(TSCarouselConfig.container);\r\n    else {\r\n        creative.forEachScreen(function (screen) {\r\n            var w = screen.deepGetEos(TSCarouselConfig.container);\r\n\r\n            if (w) wrapper = w;\r\n        });\r\n    }\r\n\r\n    var list = wrapper.element.eos;\r\n\r\n    list.forEach(function (slide, i) {\r\n        var config = slide.getConfig(currentState)\r\n\r\n        config.x = (i == 0) ?\r\n            TSCarouselConfig.slide.sizes.margin / 2 :\r\n            TSCarouselConfig.slide.sizes.margin / 2 + (widthWithMargin) * i;\r\n        config.y = 0;\r\n\r\n        (TSCarouselConfig.loop && (i == list.length - 1)) && (config.x = TSCarouselConfig.slide.sizes.margin / 2 - widthWithMargin);\r\n\r\n        bnt.ElementRendererRegistry.rendererFor(slide.element).applyScreenConfig(currentState, config);\r\n    })\r\n\r\n    var configWrapper = wrapper.getConfig(currentState);\r\n    configWrapper.x = (TSCarouselConfig.loop) ? halfXDifference : 0;\r\n    bnt.ElementRendererRegistry.rendererFor(wrapper.element).applyScreenConfig(currentState, configWrapper);\r\n};\r\n\r\nTSCarousel.Orientation = {\r\n    Landscape: 0,\r\n    Portrait: 1\r\n};\r\n\r\nTSCarousel.Position = {\r\n    TopLeft: 0,\r\n    Center: 1,\r\n    BottomRight: 2\r\n};\r\n\r\nvar TSCreativeScale = {\r\n    scale: 1,\r\n    invScale: 1,\r\n\r\n    updateScale: function () {\r\n        var state = bnt.get(bnt.State);\r\n        var rect = adController.renderer.mainStage.htmlElement.getBoundingClientRect();\r\n        var canvasSize = Math.max(state.canvas.config.width, state.canvas.config.height);\r\n        var realSize = Math.max(rect.width, rect.height);\r\n\r\n        this.scale = realSize / canvasSize;\r\n        this.invScale = 1 / this.scale;\r\n    }\r\n};\r\n\r\nvar carousel\r\nadController.onstart.addObserver(function () {\r\n    carousel = new TSCarousel(TSCarouselConfig);\r\n    carousel.initialize();\r\n\r\n    carousel.start();\r\n});\r\n\r\n/* TEADS API CALLS */\r\nbnt.TeadsPlayerAddons.apiProxy.addObserver(function (api) {\r\n    if (api) {\r\n        api.getStudioData().map(function (data) {\r\n            if (data) data.display = true\r\n            else data = { display: true }\r\n            api.setStudioData(data).map(function () {\r\n                var state = bnt.get(bnt.State);\r\n                if (state) {\r\n                    api.sendVideoMetadata({ width: state.canvas.config.width, height: state.canvas.config.height });\r\n                } else {\r\n                    var fixStage = function (state) {\r\n                        api.sendVideoMetadata({ width: state.canvas.config.width, height: state.canvas.config.height }) // force player to resize slot in case it got a different size from the vast tag\r\n                        bnt.get(bnt.StateChangeDetector).stateUpdated.removeObserver(fixStage) // we do this only once - \r\n                    };\r\n                    bnt.get(bnt.StateChangeDetector).stateUpdated.addObserver(fixStage);\r\n                }\r\n            })\r\n        })\r\n\r\n        bnt.get(bnt.ResizeController).resize.addObserver(function (dim) {\r\n            api.isFullscreen().map(function (isFs) {\r\n                adController.fullscreen.update(isFs)\r\n            })\r\n        })\r\n    }\r\n})\r\n/* END TEADS API CALLS */\r\n\r\n/* HIDE VIDEOSLOT */\r\nTSCarouselConfig.screen.onshow.addEventListener(function () {\r\n    if (adController.videoSlot) {\r\n        var videoslotContainer = document.createElement('div')\r\n        document.body.appendChild(videoslotContainer)\r\n        videoslotContainer.appendChild(adController.videoSlot)\r\n        videoslotContainer.style.display = 'none';\r\n    }\r\n});\r\n/* END HIDE VIDEOSLOT */\r\n\r\n/* CAROUSEL PRE-RENDER */\r\nif (typeof bntAd !== 'undefined' && bntAd) {\r\n    bntAd.subscribe(function (e) {\r\n        //AdLoaded\r\n        bnt.logger.log('VPAID ADLOADED')\r\n        adController.renderer.render(adController.element).map(function () {\r\n            document.body.firstChild.hidden = false;\r\n            bnt.get(bnt.MainStage).renderersMap.get(TSCarouselConfig.screen).screenElement.hidden = false;\r\n            document.getElementById(TSCarouselConfig.container.elementId).hidden = false;\r\n            var recursiveShow = function (element) {\r\n                document.getElementById(element.elementId).hidden = false;\r\n                if (element.eos) element.eos.map(function (eos) { return eos.element; }).forEach(recursiveShow);\r\n            }\r\n            TSCarouselConfig.container.eos.forEach(function (eos) {\r\n                recursiveShow(eos.element);\r\n            })\r\n\r\n            updatePositionsForPrerender(bnt.get(bnt.State));\r\n\r\n            bnt.TeadsPlayerAddons.apiProxy.addObserver(function (api) {\r\n                if (api) {\r\n                    if (typeof api['setStartScreenVisibility'] == 'function') {\r\n                        api.setStartScreenVisibility(false);\r\n                    }\r\n                }\r\n            })\r\n        })\r\n    }, 'AdLoaded')\r\n\r\n    TSCarouselConfig.screen.onshow.addObserver(function () {\r\n        bnt.get(bnt.MainStage).htmlElement.hidden = true;\r\n        bnt.get(bnt.MainStage).htmlElement = document.body.querySelector('div');\r\n\r\n        TSCreativeScale.updateScale();\r\n    })\r\n}\r\n/* END CAROUSEL PRE-RENDER */\r\n\r\n/* CAROUSEL PAUSE/RESUME */\r\nadController.onsuspend.addObserver(function () {\r\n    carousel.paused = true;\r\n})\r\nadController.onresume.addObserver(function () {\r\n    carousel.paused = false;\r\n})\r\n/* END CAROUSEL PAUSE/RESUME */\n\n\n\n}";

var bntAd = new adaptors.VpaidAd();
bntAd.setup(adsrc);

function getVPAIDAd() {return bntAd;}