        function Context() {
        }
        Context.isDesktop = function () {
            return !Context.isMobile() && !Context.isTablet();
        };
        Context.isIpad = function () {
            return Context.getDevice().model === 'iPad' || /iPad/i.test(Context.getUA());
        };
